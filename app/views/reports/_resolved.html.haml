.report.eliminated
  .row
    .rp_report_status.status
      .rp_report_options{:style => "margin-top:-3px;"}
        = render :partial => "reports/topbar", :locals => {:report => report}

      .span{:style => "float:right;"}
        = I18n.t("common_terms.eliminated")

  .row
    .span6{:style => "margin-top:10px;"}
      %h4= report.complete_address
    .span2
      - if report.location.latitude and report.location.longitude
        = link_to I18n.t("common_terms.find_on_map"), "#", id: "mapa_#{report.id}", class: "mapa", data: { latitude: "#{report.location.latitude}", longitude: "#{report.location.longitude}"}, style: "margin-top:5px;float:right;border:1px solid gray;margin-right:-6px;padding:2px;"

  .row
    = render :partial => "reports/attributes", :locals => {:report => report}
