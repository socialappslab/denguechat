- dashboard_navigation_menu.each do |md_navigation|
  .master-view-cell{:class => (md_navigation[1] == request.path ? "active" : "")}
    = link_to md_navigation[1] do
      .row
        .col-md-2.text-center
          %i.fa{:class => md_navigation[2]}
        .col-md-9
          = md_navigation[0]
